{
  "name": "metal-flux",
  "version": "1.0.0",
  "description": "A Flux implementation for Metal.js",
  "license": "MIT",
  "repository": "metal/metal-flux",
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "jsnext:main": "src/all/flux.js",
  "main": "lib/all/flux.js",
  "files": [
    "lib",
    "src/**/*.js",
    "test/**/*.js"
  ],
  "scripts": {
